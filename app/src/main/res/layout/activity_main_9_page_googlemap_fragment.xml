<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context="com.example.louis.no_find_you.MainActivity_8_page_Profile">

    <include
        layout="@layout/main_1_main_toolbar_custom"
        android:id="@+id/main_1_main_toolbar_custom"/>


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp"
        android:weightSum="10"
        >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="8">


            <android.support.design.widget.CoordinatorLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/border_main_1"
                >

                <fragment
                    android:id="@+id/main_9_fragment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:name="com.google.android.gms.maps.MapFragment"
                    />

                <android.support.design.widget.FloatingActionButton
                android:id="@+id/animation_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|end"
                android:layout_marginRight="@dimen/activity_vertical_margin"
                android:layout_marginEnd="@dimen/animation_margin"
                android:layout_marginBottom="160dp"
                android:src="@drawable/z_ic_location_24dp"
                app:backgroundTint="@color/purple_2"
                app:fabSize="normal"
                android:visibility="invisible"
                />

                <android.support.design.widget.FloatingActionButton
                android:id="@+id/animation_person_outline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|end"
                android:layout_marginRight="@dimen/animation_margin"
                android:layout_marginEnd="@dimen/animation_margin"
                android:layout_marginBottom="90dp"
                android:src="@drawable/z_ic_person_black_24dp"
                app:fabSize="normal"
                app:backgroundTint="@color/purple_2"
                android:visibility="invisible"
                />

                <android.support.design.widget.FloatingActionButton
                android:id="@+id/animation_plus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|end"
                android:layout_margin="16dp"
                android:src="@drawable/z_ic_add_black_24dp"
                app:backgroundTint="@color/purple_2"
                app:fabSize="normal"
                />
            </android.support.design.widget.CoordinatorLayout>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2">
    </LinearLayout>




</LinearLayout>
